@article{ WOS:000271829100006,
Author = {Becker, Birgit},
Title = {Which children attend preschool earlier? A comparison between German and
   Turkish families},
Journal = {ZEITSCHRIFT FUR SOZIOLOGIE DER ERZIEHUNG UND SOZIALISATION},
Year = {2009},
Volume = {29},
Number = {4},
Pages = {387-402},
Abstract = {Children already differ in their educational prospects when they start
   school. Attending a child care center is often seen as an adequate means
   for compensation because such formal child care settings can yield
   positive effects on different skills. However, the starting time and
   therefore the duration of preschool attendance are probably selective.
   This selectivity in the timing of preschool attendance is analyzed in
   this article by means of survival analysis. The empirical analyses are
   conducted with data of the project `Preschool Education and Educational
   Careers among Migrant Children' in which German and Turkish parents of
   3-4-year-old children were surveyed. On average, the German children
   start preschool earlier than the children with a Turkish migration
   background. Knowledge about the institution `Kindergarten' proves to be
   an important determinant. The existence of role models in the social
   network has a significant effect only in the Turkish sample. In Turkish
   families, children start preschool earlier if their parents have a good
   knowledge of German.},
