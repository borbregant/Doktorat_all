@article{ WOS:000287292700012,
Author = {Glocker, Daniela},
Title = {The effect of student aid on the duration of study},
Journal = {ECONOMICS OF EDUCATION REVIEW},
Year = {2011},
Volume = {30},
Number = {1},
Pages = {177-190},
Month = {FEB},
Abstract = {In this paper I evaluate the effect of student aid on the success of
   academic studies. I focus on two dimensions, the duration of study and
   the probability of actually graduating with a degree. To determine the
   impact of financial student aid, I estimate a discrete-time duration
   model allowing for competing risks to account for different exit states
   (graduation and dropout) using individual level panel data from the
   German Socio-Economic Panel (SOEP) for the years 1984-2007. My findings
   suggest that the duration of study is responsive to the type of
   financial support a student receives. There are three main results.
   First, student aid recipients finish faster than comparable students who
   are supported by the same amount of parental/private transfers only.
   Second, although higher financial aid does on average not affect the
   duration of study, this effect is (third) dominated by the increased
   probability of actually finishing university successfully. (c) 2010
   Elsevier Ltd. All rights reserved.},
