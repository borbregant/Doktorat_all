@article{ WOS:000302011500014,
Author = {Lima Junior, Paulo and da Silveira, Fernando Lang and Ostermann,
   Fernanda},
Title = {Survival analysis applied to student flow in undergraduate Physics
   courses: an example from a Brazilian university},
Journal = {REVISTA BRASILEIRA DE ENSINO DE FISICA},
Year = {2012},
Volume = {34},
Number = {1},
Month = {JAN-MAR},
Abstract = {Survival analysis is a statistical method widely used in health sciences
   with applications in other fields. Its object of study is the time
   between events, usually called ``survival time{''}. This paper
   investigates the possibility of applying survival analysis to study
   students' retention, dropout and degree in an undergraduate Physics
   course. As a result, it is possible to notice that survival analysis is
   a suitable method, enabling one to describe when dropouts and degrees
   are more likely to occur and what factors are related to retention in
   this course. The goal of this work is to make a brief introduction to
   survival analysis, illustrating its application to the study of students
   flow from a Physics course in a Brazilian university. We believe this
   paper is an actual methodological contribution to researchers interested
   in the topic of dropout and retention in science education since
   survival analysis is an efficient tool for describing and understanding
   student flow.},
