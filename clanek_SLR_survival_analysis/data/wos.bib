@article{ WOS:000267654900016,
Author = {Ries, Andrew and Wingard, Deborah and Morgan, Cindy and Farrell,
   Elizabeth and Letter, Sharon and Reznik, Vivian},
Title = {Retention of Junior Faculty in Academic Medicine at the University of
   California, San Diego},
Journal = {ACADEMIC MEDICINE},
