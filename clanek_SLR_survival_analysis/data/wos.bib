@article{ WOS:000400659700008,
Author = {Meggiolaro, Silvia and Giraldo, Anna and Clerici, Renata},
Title = {A multilevel competing risks model for analysis of university students'
   careers in Italy},
Journal = {STUDIES IN HIGHER EDUCATION},
Year = {2017},
Volume = {42},
Number = {7},
Pages = {1259-1274},
Abstract = {This paper examines individual and institutional characteristics which
   may influence the outcomes of university students' careers. Withdrawals,
   course changes, delays and graduations of students enrolled in
   first-cycle degree courses in a large public university in Italy are
   examined. Individual longitudinal data from administrative archives were
   used, taking into account both the temporal dimension and the
   organisational and structural characteristics of the degree courses.
   Results indicate that the profile of a successful student is defined by
   both socio-demographic factors and pre-university educational
   experience. At course level, restricted access to courses, study fields
   and course size were important for students' university careers.},
