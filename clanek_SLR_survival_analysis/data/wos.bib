Affiliation = {Chen, R (Corresponding Author), Seton Hall Univ, Coll Educ \& Human Serv, Jubilee Hall,400 S Orange Ave, S Orange, NJ 07079 USA.
   Chen, Rong, Seton Hall Univ, Coll Educ \& Human Serv, S Orange, NJ 07079 USA.
   DesJardins, Stephen L., Univ Michigan, Sch Educ, Ann Arbor, MI 48109 USA.},
DOI = {10.1007/s11162-007-9060-9},
ISSN = {0361-0365},
