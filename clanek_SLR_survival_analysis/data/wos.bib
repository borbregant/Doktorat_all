@article{ WOS:000241416300027,
Author = {De Champlain, Andre and Sample, Laurel and Dillon, Gerard F. and Boulet,
   John R.},
Title = {Modeling longitudinal performances on the United States Medical
   Licensing Examination and the impact of sociodemographic covariates: An
   application of survival data analysis},
