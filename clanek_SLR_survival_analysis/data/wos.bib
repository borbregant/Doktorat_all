@article{ WOS:000657286300001,
Author = {Elsayed, Mahmoud A. A. and Roch, Christine H.},
Title = {Former Teachers: Exits and Re-Entries},
Journal = {EDUCATIONAL POLICY},
Year = {2023},
Volume = {37},
Number = {2},
Pages = {279-307},
Month = {MAR},
Abstract = {Despite the large literature on teacher labor market in the United
   States, only few studies have examined the career choices of former
   teachers and the factors that affect their decisions to return to the
   profession. This is surprising given that former teachers represent over
   a third of teachers entering the teaching workforce, according to some
   estimates. This paper examines the exit and re-entry decisions of former
   teachers using a restricted-use data from the Beginning Teacher
   Longitudinal Study (BTLS). We use a discrete time hazard model that
   estimates the probability that a former teacher returns to teaching in a
   given year conditional on not having returned in the previous year.
   Results suggest that female teachers are more likely to return to the
   teaching profession by somewhere between 10 and 12 percentage points. We
   also find that teachers who are highly paid are more likely to re-enter
   teaching.},
