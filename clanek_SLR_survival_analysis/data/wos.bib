@article{ WOS:001139994200001,
Author = {Dehon, Catherine and Lebouteiller, Leonore},
Title = {A comparison between two systems of university education: years of study
   versus credit accumulation},
Journal = {EDUCATION ECONOMICS},
Year = {2024},
Month = {2024 JAN 12},
Abstract = {A recent major reform of higher education has changed a year-based
   system into a credit accumulation one. We use this exogenous shock to
   compare the two systems. Using a longitudinal dataset of undergraduate
   students from one of the largest French-speaking universities, a
   competing risks survival analysis is performed to study the
   probabilities and duration to graduation or dropout. The results
   indicate that the credit accumulation system encourages students to be
   less engaged in their studies, at the cost of late graduation in the
   best case, and in the worst case, late dropout which affects mostly
   students coming from disadvantaged backgrounds.},
