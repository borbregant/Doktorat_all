@article{ WOS:000248116700002,
Author = {Calcagno, Juan Carlos and Crosta, Peter and Bailey, Thomas and Jenkins,
   Davis},
Title = {Stepping stones to a degree: The impact of enrollment pathways and
   milestones on community college student outcomes},
